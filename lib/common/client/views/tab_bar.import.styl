.tab-bar
  .tabs
    height 60px
    background-color  transparent
    border none
    a
      position relative
      padding-top 11px
      z-index 2
    .add-button
      position absolute
      top 0
      bottom 2em
      left 0
      right 0
      margin auto
      width 3em
    .bottom-nav,.bottom-nav-in
      position  absolute
      left 0
      top 0
      width  100%
      height  100%
      background-color  #2f5177
      background-image -webkit-linear-gradient( 45deg, #1e314c 0%,#2f5177 15%,#325c89 20%,#4e78a8 50%,#325c89 80%,#2f5177 85%,#1e314c 100%)
      background-image -o-linear-gradient(45deg, #1e314c 0%,#2f5177 15%,#325c89 20%,#4e78a8 50%,#325c89 80%,#2f5177 85%,#1e314c 100%)
      background-image -moz-linear-gradient(45deg, #1e314c 0%,#2f5177 15%,#325c89 20%,#4e78a8 50%,#325c89 80%,#2f5177 85%,#1e314c 100%)
      background-image -ms-linear-gradient(45deg, #1e314c 0%,#2f5177 15%,#325c89 20%,#4e78a8 50%,#325c89 80%,#2f5177 85%,#1e314c 100%)
      background-image linear-gradient(45deg, #1e314c 0%,#2f5177 15%,#325c89 20%,#4e78a8 50%,#325c89 80%,#2f5177 85%,#1e314c 100%)
      z-index  1
      .mask-ref
        position  absolute
        bottom  0
        left  0
        width  100%
        height  50%
        background-color  rgba(0,0,0,0.08)
        z-index  6
      .mask-top
        position  absolute
        left  0
        top  -2px
        width  100%
        height  8px
        background-color  #2f5177
        background-image -webkit-linear-gradient( top, #efefef 0%,#838383 100%)
        background-image -o-linear-gradient(top, #efefef 0%,#838383 100%)
        background-image -moz-linear-gradient(top, #efefef 0%,#838383 100%)
        background-image -ms-linear-gradient(top, #efefef 0%,#838383 100%)
        background-image linear-gradient(top, #efefef 0%,#838383 100%)
        z-index 2
      .add-area
        position  absolute
        top  0
        left 50%
        width  20%
        height  65%
        margin-left  -10%
        border  1px solid #283f5a
        border-bottom  0
        border-top-left-radius  10px
        border-top-right-radius  10px
        overflow  hidden
        z-index  3
        .bottom-nav-in
          position  absolute
          left -200%
          top  0
          width  500%
          height  155%
      .add-shadow-left
        position  absolute
        left  38.5%
        top  0
        width  2%
        min-width  12px
        height  64%
        background-image -webkit-linear-gradient( to left, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -o-linear-gradient(to left, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -moz-linear-gradient(to left, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -ms-linear-gradient(to left, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image linear-gradient(to left, rgba(0,0,0,0.5) 0%,transparent 100%)
        transform rotate(-15deg)
        -webkit-transform rotate(-15deg)
        -moz-transform rotate(-15deg)
        -o-transform rotate(-15deg)
        -ms-transform rotate(-15deg)
        z-index  1
      .add-shadow-right
        position  absolute
        right  38.5%
        top  0
        width  2%
        min-width  12px
        height  64%
        background-image -webkit-linear-gradient( to right, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -o-linear-gradient(to right, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -moz-linear-gradient(to right, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image -ms-linear-gradient(to right, rgba(0,0,0,0.5) 0%,transparent 100%)
        background-image linear-gradient(to right, rgba(0,0,0,0.5) 0%,transparent 100%)
        transform rotate(15deg)
        -webkit-transform rotate(15deg)
        -moz-transform rotate(15deg)
        -o-transform rotate(15deg)
        -ms-transform rotate(15deg)
        z-index  1
      .add-left,.add-right
        position  absolute
        top  20%
        width  1px
        height 50%
        background-image -webkit-linear-gradient( top, #283f5a 0%,#375a80 60%,#3e6a99 100%)
        background-image -o-linear-gradient(top, #283f5a 0%,#375a80 60%,#3e6a99 100%)
        background-image -moz-linear-gradient(top, #283f5a 0%,#375a80 60%,#3e6a99 100%)
        background-image -ms-linear-gradient(top, #283f5a 0%,#375a80 60%,#3e6a99 100%)
        background-image linear-gradient(top, #283f5a 0%,#375a80 60%,#3e6a99 100%)
        z-index 4
      .add-left
        left  40%
      .add-right
        right  40%
      .add-shadow-top
        position  absolute
        width  100%
        height 15%
        top  6px
        background-image -webkit-linear-gradient(to  top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -o-linear-gradient(to top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -moz-linear-gradient(to top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -ms-linear-gradient(to top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image linear-gradient(to top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        z-index  1
      .add-shadow-bottom
        position  absolute
        width  100%
        height 15%
        bottom  0
        background-image -webkit-linear-gradient( top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -o-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -moz-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image -ms-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        background-image linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.1) 100%)
        z-index  1