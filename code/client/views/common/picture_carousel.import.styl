.picture-carousel
  height 200px
  .ion-slide-box
    height 100%
    .slick-list
      height 100%
      .slick-track
        height 100%
        .slick-slide
          height 100%
          background-color #ccc
          display flex
          .picture
            max-width 100%
            max-height 100%
            width auto
            height auto
            margin auto