@require 'nib'

.note-detail
  size 100%
  #picture-carousel
    height 200px
    .slick-list
      height 100%
      .slick-track
        height 100%
        .slick-slide
          height 100%
          background-color blue
          img
            max-width 100%
            max-height 100%
            width auto
            height auto
            clear both
            display block
            margin auto